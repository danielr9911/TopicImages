<div class="container">

  <!-- Page Heading -->
  <div class="row">
    <div class="col-lg-12">
      <h1 class="header">Mi contenido
        <small>Subido por ti</small>
      </h1>
    </div>
  </div>
  <!-- /.row -->
  <table class="table">
    <tr>
      <th>Titulo</th>
      <th>Privado</th>
      <th>Imagen</th>
      <th>Acciones</th>
    </tr>
    {{#each imagenes}}
      <tr>
        <td>{{this.titulo}}</td>
        <td>{{this.privado}}</td>
        <td><a href="{{this.imagen}}" class="btn">Ver imagen</a></td>
        <td>
          <a href="/edit/{{this.id}}" class="btn">Editar</a>
        </td>
      </tr>
    {{/each}}
  </table>


</div>

